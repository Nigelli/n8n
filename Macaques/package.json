{
  "name": "@n8n-soar/macaques",
  "version": "0.1.0",
  "description": "Plugin system for extending n8n without modifying upstream code",
  "type": "module",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./frontend": {
      "types": "./dist/frontend/index.d.ts",
      "import": "./dist/frontend/index.js"
    },
    "./backend": {
      "types": "./dist/backend/index.d.ts",
      "import": "./dist/backend/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "typecheck": "tsc --noEmit",
    "validate": "tsx src/cli/validate.ts"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "dependencies": {
    "fast-glob": "^3.3.2",
    "picocolors": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.0",
    "typescript": "^5.9.0",
    "vite": "^6.0.0"
  },
  "peerDependencies": {
    "vite": ">=5.0.0",
    "@n8n/di": "*"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "@n8n/di": {
      "optional": true
    }
  }
}
